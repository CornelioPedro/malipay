$(document).ready(function(){$("#divStartApplicationPopup").modal({backdrop:"static",keyboard:!0,show:!1});$(".start-application-modal-trigger").on("click",function(n){return n.preventDefault(),n.stopPropagation(),$("#divStartApplicationPopup").modal("show"),$("html, body").animate({scrollTop:$("body").offset().top},2e3),$(".navbar-toggle").length>0&&$(".navbar-toggle").each(function(){$(this).hasClass("collapsed")||$(this).click()}),!1})})