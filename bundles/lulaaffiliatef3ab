var lula=function(){function t(){$(function(){var t="";n.getParam("a")!=undefined&&(t="a="+unescape(n.getParam("a")));n.getParam("c")!=undefined&&(t="c="+unescape(n.getParam("c")));t!=""&&$.cookie("LulaLendAffiliate",t,{secure:!0})})}var n={addParams:function(n,t){var i=document.location.search.substr(1).split("&"),r,e;for(i.length==1&&i[0]==""&&(i=[]),r=0;r<n.length;r++){for(var o=encodeURI(n[r].key),s=encodeURI(n[r].value),u=i.length,f;u--;)if(f=i[u].split("="),f[0]==o){f[1]=s;i[u]=f.join("=");break}u<0&&(i[i.length]=[o,s].join("="))}t?document.location.search=i.join("&"):history.pushState&&(e=window.location.protocol+"//"+window.location.host+window.location.pathname+"?"+i.join("&"),window.history.pushState({path:e},"",e))},updateParams:function(t,i,r){if(i)for(var u=0;u<i.length;u++)n.removeParam(i[u].key,!1);t&&n.addParams(t,r)},addParam:function(t,i,r){n.addParams([{key:t,value:i}],r)},removeParam:function(n,t){var e=encodeURI(n),i=document.location.search.substr(1).split("&"),r,f,u;for(i.length==1&&i[0]==""&&(i=[]),r=i.length;r--;)if(f=i[r].split("="),f[0]==e){i.splice(r,1);break}t?document.location.search=i.join("&"):history.pushState&&(u=window.location.protocol+"//"+window.location.host+window.location.pathname+"?"+i.join("&"),window.history.pushState({path:u},"",u))},getParam:function(n){for(var i,u=window.location.search.substring(1),r=u.split("&"),t=0;t<r.length;t++)if(i=r[t].split("="),i[0]==n)return i[1];return undefined}};return t(),{queryString:n}}(jQuery)