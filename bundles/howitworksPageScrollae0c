function pageScrollClick(){try{var n=window.location.hash.replace("#","");n&&$("#show"+n.toLowerCase()).click()}catch(t){}}function defaultScrollAnimation(n){$("html, body").animate({scrollTop:$(n).offset().top-50},2e3)}$(document).ready(function(){$("#showdoyouqualify").on("click",function(){return defaultScrollAnimation($(this)),!1});$("#showslider").on("click",function(){return defaultScrollAnimation($(this)),!1});$(".calcTriggerHowItWorks").on("click",function(){return $("#showslider").click(),!1});pageScrollClick()})