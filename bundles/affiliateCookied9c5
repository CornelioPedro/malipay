function CreateAffiliateCookie(n,t,i){var r=new Date,u;r.setTime(r.getTime()+i*864e5);u="expires="+r.toUTCString();document.cookie=n+"="+t+u+"; path=/"}function checkAffiliateCookie(n){var i="LulaLendAffiliate",u=getCookie(i),r,t;u==""?CreateAffiliateCookie(i,n,30):(r=30,t=new Date,t.setTime(t.getTime()+r*864e5),$.cookie(i,n,{expires:t}))}function getCookie(n){for(var t,r=n+"=",f=decodeURIComponent(document.cookie),u=f.split(";"),i=0;i<u.length;i++){for(t=u[i];t.charAt(0)==" ";)t=t.substring(1);if(t.indexOf(r)==0)return t.substring(r.length,t.length)}return""}